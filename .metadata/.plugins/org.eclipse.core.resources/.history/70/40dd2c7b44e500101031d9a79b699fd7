package iFexample;

import java.util.Scanner;

public class ifEx01 {

	public static void main(String[] args) {
		// 
		Scanner scan = new Scanner(System.in);
		System.out.print("ID입력 : ");
		String dbid = "백구";
		dbid = scan.next();
		System.out.print("Pw입력 : ");
		String dbPw = "황구";
		dbPw = scan.next();
		if(dbid=="백구" && dbPw == "황구") {
			System.out.println("로그인 성공");
		}else {
			System.out.println("로그인 실패");
		}
	}

}
